{
  "name": "webpack-e",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@material-ui/core": "^3.9.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "scripts": {
    "start": "webpack-dev-server --colors --progress --env.mode development --watch-poll ",
    "build": "npm run clean && webpack --progress --colors --mode=production",
    "analyze:prod": "npm run clean && webpack --progress --colors --env.mode production --env.presets analyze",
    "test": "jest",
    "test:cover": "npm run test -- -env=jsdom --watchAll --coverage && exit 0",
    "test:watch": "jest --watch",
    "lint:scss": "stylelint --config ./stylelint.config.js  --syntax scss \"./src/**/*.scss\"",
    "lint:ts": "eslint \"src/**/*.{ts,tsx}\"",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit",
    "stylefix": "stylefmt --syntax scss --config ./stylelint.config.js --recursive \"./src/**/*.scss\"",
    "clean": "rm -fr ./build",
    "prettier": "pretty-quick --staged",
    "format": "prettier-eslint --write \"src/**/*.{ts,tsx}\"",
    "kill:3000": "kill $(lsof -t -i:3000)",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/dotenv": "^6.1.0",
    "@types/enzyme": "^3.1.16",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.21",
    "@types/node-fetch": "^2.1.4",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/webpack": "^4.4.24",
    "@types/webpack-env": "latest",
    "@typescript-eslint/eslint-plugin": "^1.2.0",
    "@typescript-eslint/parser": "^1.2.0",
    "@typescript-eslint/typescript-estree": "^1.2.0",
    "arui-presets": "^5.0.2",
    "autoprefixer": "^9.4.6",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "dotenv": "^6.2.0",
    "dotenv-expand": "^4.2.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.0.1",
    "express": "^4.16.4",
    "express-static-gzip": "^1.1.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "fs-extra": "7.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "nodemon-webpack-plugin": "^4.0.7",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.14",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-reporter": "^6.0.1",
    "postcss-safe-parser": "^4.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "pretty-quick": "^1.10.0",
    "react-dev-utils": "^7.0.1",
    "resolve": "^1.10.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylefmt": "^6.0.3",
    "stylelint-scss": "^3.5.3",
    "terser-webpack-plugin": "^1.2.2",
    "ts-jest": "^23.10.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.12.1",
    "typescript": "^3.2.1",
    "typescript-formatter": "^7.2.2",
    "url-loader": "^1.1.2",
    "utility-types": "^3.4.1",
    "webpack": "^4.29.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn ts-compile-check && yarn test && lint-staged"
    }
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "src/**/*.{jsx,js}": [
        "npm run format",
        "jest --bail --findRelatedTests"
      ],
      "src/**/*.{css}": "stylelint ./stylelint.config.js",
      "src/**/*.{scss}": [
        "npm run stylefix",
        "git add"
      ],
      "src/**/*.{tsx,ts}": [
        "npm run format",
        "jest --bail --findRelatedTests"
      ]
    }
  }
}

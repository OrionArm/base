    "url-loader": "^1.1.2",
    "utility-types": "^3.4.1",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^3.6.3"
  }
}

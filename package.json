{
  "name": "webpack-e",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@material-ui/core": "^3.9.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "scripts": {
    "start": "webpack-dev-server --colors --progress --env.mode development --watch-poll",
    "build:dev": "npm run clean && webpack --progress --colors --env.mode development",
    "build:prod": "npm run clean && webpack --progress --colors --env.mode production",
    "analyze:prod": "npm run clean && webpack --progress --colors --env.mode production --env.presets analyze",
    "test": "jest",
    "test:cover": "npm run test -- -env=jsdom --watchAll --coverage && exit 0",
    "test:watch": "jest --watch",
    "lint:scss": "stylelint --config ./stylelint.config.js  --syntax scss \"./src/**/*.scss\"",
    "lint:ts": "eslint \"src/**/*.{ts,tsx}\"",
    "ts-compile-check": "tsc -p tsconfig.json --noEmit",
    "lintfix": "npm run lintfix:ts; npm run lintfix:scss",
    "lintfix:ts": "eslint --fix",
    "lintfix:scss": "stylefmt --syntax scss --config ./stylelint.config.js --recursive \"./src/**/*.scss\"",
    "clean": "rm -fr ./build",
    "prettier": "pretty-quick --staged",
    "kill:3000": "kill $(lsof -t -i:3000)",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.3",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/webpack": "^4.4.23",
    "@types/webpack-env": "latest",
    "@typescript-eslint/eslint-plugin": "^1.1.1",
    "@typescript-eslint/parser": "^1.1.1",
    "@typescript-eslint/typescript-estree": "^1.1.1",
    "arui-presets": "^5.0.2",
    "autoprefixer": "^9.4.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-preset-react-app": "^7.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "dotenv": "^6.2.0",
    "dotenv-expand": "^4.2.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.1",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "fs-extra": "7.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.13",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "postcss-reporter": "^6.0.1",
    "postcss-safe-parser": "^4.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.16.0",
    "pretty-quick": "^1.10.0",
    "react-dev-utils": "^7.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylefmt": "^6.0.3",
    "stylelint-scss": "^3.5.1",
    "terser-webpack-plugin": "^1.1.0",
    "ts-jest": "^23.10.5",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
